<%
const blogPage = _.cms('capas')
const { slug, content } = await _.resolveRoute()
_.breadcrumbs = [{
  name: 'Blow Gummies',
  link: `/blow-gummies-teste`
}]
//console.log(content)
if (!content.sections || !content.sections.length) {
  content.sections = [
    { type: 'breadcrumbs' },
    { type: 'page-title' },
    { type: 'encontre-sua-capa' }
  ]
} else if (!content.sections.find(({ type }) => type === 'blow-gummies-teste')) {
  content.sections.push({ type: 'blow-gummies-teste' })
}
%>

<%- await include('@/view') %>